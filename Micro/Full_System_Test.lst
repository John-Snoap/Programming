0152                         * ;Interrupt Service Routine											COMMENTS
0153 e11e 0e                 ISR				CLI										;Clear the interrupt flag
0154 e11f b6 a0 00           				LDAA		keyboard							;Load keypress
0155 e122 84 1f              				ANDA		#$1F								;Clear the first three bits
0156 e124 9b a2              				ADDA		shiftFlag							;Add shift
0157 e126 81 03              				CMPA		#%11								;if (keypress == shift && !shiftFlag)
0158 e128 27 3f              				BEQ		shiftOn								;shiftFlag = on
0159 e12a 81 13              				CMPA		#%10011								;else if (keypress == mode && !shiftFlag)
0160 e12c 27 45              				BEQ		modePlusPlus							;mode++
0161 e12e 81 23              				CMPA		#%100011							;else if (keypress == shift && shiftFlag)
0162 e130 27 3c              				BEQ		shiftOff							;shiftFlag = off
0163 e132 81 33              				CMPA		#%110011							;else if (keypress == mode && shiftFlag)
0164 e134 27 4d              				BEQ		modeMinusMinus							;mode--
0165 e136 ce 00 b0           				LDX		#bufferStart							;else, keypress is a character
0166 e139 d6 a0              				LDAB		characterCount							;characterCount -> B
0167 e13b 3a                 				ABX										;Add character count to buffer start
0168 e13c a7 00              				STAA		0,X								;Keypress -> buffer
0169 e13e ce e4 00           				LDX		#LCDStart							;Start LCD translation
0170 e141 16                 				TAB										;Keypress -> B
0171 e142 3a                 				ABX										;Add keypress to LCD start
0172 e143 a6 00              				LDAA		0,X								;Finish LCD translation
0173 e145 ce b0 00           				LDX		#LCD								;Prepare to send LCD translation to LCD
0174 e148 1e 00 80 fc        				BRSET		0,X			%10000000	*			;LCD busy check
0175 e14c b7 b0 01           				STAA		LCDD								;LCD translation -> LCD
0176 e14f 7c 00 a0           				INC		characterCount							;characterCount++
0177 e152 86 05              				LDAA		#5								;5 -> A
0178 e154 d6 a0              				LDAB		characterCount							;characterCount -> B
0179 e156 11                 				CBA										;if (characterCount < 5)
0180 e157 26 0f              				BNE		done								;Go to done
0181 e159 7f 00 a0           				CLR		characterCount							;else, characterCount = 0  Need to add clear display
0182 e15c ce b0 00           				LDX		#LCD								;LCD location -> X		Clear LCD
0183 e15f 1e 00 80 fc        				BRSET		0,X			%10000000	*			;LCD busy check
0184 e163 86 01              				LDAA		#$1								;Load first command
0185 e165 b7 b0 00           				STAA		LCD								;Send first command -> LCD
0186 e168 3b                 done				RTI										;Return from interrupt
0187 e169 14 a2 20           shiftOn				BSET		shiftFlag		%100000					;Shift on
0188 e16c 20 fa              				BRA		done								;Go to done
0189 e16e 15 a2 20           shiftOff			BCLR		shiftFlag		%100000					;Shift off
0190 e171 20 f5              				BRA		done								;Go to done
0191 e173 7c 00 a3           modePlusPlus			INC		mode								;mode++
0192 e176 86 05              				LDAA		#5								;5 -> A
0193 e178 d6 a3              				LDAB		mode								;mode -> B
0194 e17a 11                 				CBA										;if (mode < 5)
0195 e17b 26 eb              				BNE		done								;Go to done
0196 e17d 86 01              				LDAA		#1								;else
0197 e17f 97 a3              				STAA		mode								;mode = 1
0198 e181 20 e5              				BRA		done								;Go to done
0199 e183 7a 00 a3           modeMinusMinus			DEC		mode								;mode--
0200 e186 86 00              				LDAA		#0								;0 -> A
0201 e188 d6 a3              				LDAB		mode								;mode -> B
0202 e18a 11                 				CBA										;if (mode > 0)
0203 e18b 26 db              				BNE		done								;Go to done
0204 e18d 86 04              				LDAA		#4								;else
0205 e18f 97 a3              				STAA		mode								;mode = 4
0206 e191 20 d5              				BRA		done								;Go to done
