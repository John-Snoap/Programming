0158 e12a 9b a2              				ADDA		shiftFlag							;Add shift
0159 e12c 81 03              				CMPA		#%11								;if (keypress == shift && !shiftFlag)
0160 e12e 27 3f              				BEQ		shiftOn								;shiftFlag = on
0161 e130 81 13              				CMPA		#%10011								;else if (keypress == mode && !shiftFlag)
0162 e132 27 45              				BEQ		modePlusPlus							;mode++
0163 e134 81 23              				CMPA		#%100011							;else if (keypress == shift && shiftFlag)
0164 e136 27 3c              				BEQ		shiftOff							;shiftFlag = off
0165 e138 81 33              				CMPA		#%110011							;else if (keypress == mode && shiftFlag)
