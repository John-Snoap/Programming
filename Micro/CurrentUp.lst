0176 e14e 1e 00 80 fc        				BRSET		0,X			%10000000	*			;LCD busy check
0177 e152 b7 b0 01           				STAA		LCDD								;LCD translation -> LCD
0178 e155 7c 00 a0           				INC		characterCount							;characterCount++
0179 e158 86 05              				LDAA		#5								;5 -> A
0180 e15a d6 a0              				LDAB		characterCount							;characterCount -> B
0181 e15c 11                 				CBA										;if (characterCount < 5)
0182 e15d 26 0f              				BNE		done								;Go to done
0183 e15f 7f 00 a0           				CLR		characterCount							;else, characterCount = 0  Need to add clear display
0184 e162 ce b0 00           				LDX		#LCD								;LCD location -> X		Clear LCD
0185 e165 1e 00 80 fc        				BRSET		0,X			%10000000	*			;LCD busy check
0186 e169 86 01              				LDAA		#$1								;Load first command
0187 e16b b7 b0 00           				STAA		LCD								;Send first command -> LCD
0188 e16e 3b                 done				RTI										;Return from interrupt
0189 e16f 14 a2 20           shiftOn				BSET		shiftFlag		%100000					;Shift on
0190 e172 20 fa              				BRA		done								;Go to done
0191 e174 15 a2 20           shiftOff			BCLR		shiftFlag		%100000					;Shift off
0192 e177 20 f5              				BRA		done								;Go to done
0193 e179 7c 00 a3           modePlusPlus			INC		mode								;mode++
0194 e17c 86 05              				LDAA		#5								;5 -> A
0195 e17e d6 a3              				LDAB		mode								;mode -> B
0196 e180 11                 				CBA										;if (mode < 5)
0197 e181 26 eb              				BNE		done								;Go to done
0198 e183 86 01              				LDAA		#1								;else
0199 e185 97 a3              				STAA		mode								;mode = 1
0200 e187 20 e5              				BRA		done								;Go to done
0201 e189 7a 00 a3           modeMinusMinus			DEC		mode								;mode--
0202 e18c 86 00              				LDAA		#0								;0 -> A
0203 e18e d6 a3              				LDAB		mode								;mode -> B
0204 e190 11                 				CBA										;if (mode > 0)
0205 e191 26 db              				BNE		done								;Go to done
0206 e193 86 04              				LDAA		#4								;else
