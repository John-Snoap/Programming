0153                         * ;Interrupt Service Routine											COMMENTS
0154 e121 0e                 ISR				CLI										;Clear the interrupt flag
0155 e122 b6 a0 00           				LDAA		keyboard							;Load keypress
0156 e125 84 1f              				ANDA		#$1F								;Clear the first three bits
0157 e127 9b a2              				ADDA		shiftFlag							;Add shift
0158 e129 81 03              				CMPA		#%11								;if (keypress == shift && !shiftFlag)
0159 e12b 27 3f              				BEQ		shiftOn								;shiftFlag = on
0160 e12d 81 13              				CMPA		#%10011								;else if (keypress == mode && !shiftFlag)
0161 e12f 27 45              				BEQ		modePlusPlus							;mode++
0162 e131 81 23              				CMPA		#%100011							;else if (keypress == shift && shiftFlag)
0163 e133 27 3c              				BEQ		shiftOff							;shiftFlag = off
0164 e135 81 33              				CMPA		#%110011							;else if (keypress == mode && shiftFlag)
0165 e137 27 4d              				BEQ		modeMinusMinus							;mode--
0166 e139 ce 00 b0           				LDX		#bufferStart							;else, keypress is a character
0167 e13c d6 a0              				LDAB		characterCount							;characterCount -> B
0168 e13e 3a                 				ABX										;Add character count to buffer start
0169 e13f a7 00              				STAA		0,X								;Keypress -> buffer
0170 e141 ce e4 00           				LDX		#LCDStart							;Start LCD translation
0171 e144 16                 				TAB										;Keypress -> B
0172 e145 3a                 				ABX										;Add keypress to LCD start
0173 e146 a6 00              				LDAA		0,X								;Finish LCD translation
0174 e148 ce b0 00           				LDX		#LCD								;Prepare to send LCD translation to LCD
0175 e14b 1e 00 80 fc        				BRSET		0,X			%10000000	*			;LCD busy check
0176 e14f b7 b0 01           				STAA		LCDD								;LCD translation -> LCD
0177 e152 7c 00 a0           				INC		characterCount							;characterCount++
0178 e155 86 05              				LDAA		#5								;5 -> A
0179 e157 d6 a0              				LDAB		characterCount							;characterCount -> B
0180 e159 11                 				CBA										;if (characterCount < 5)
0181 e15a 26 0f              				BNE		done								;Go to done
0182 e15c 7f 00 a0           				CLR		characterCount							;else, characterCount = 0  Need to add clear display
0183 e15f ce b0 00           				LDX		#LCD								;LCD location -> X		Clear LCD
0184 e162 1e 00 80 fc        				BRSET		0,X			%10000000	*			;LCD busy check
0185 e166 86 01              				LDAA		#$1								;Load first command
0186 e168 b7 b0 00           				STAA		LCD								;Send first command -> LCD
0187 e16b 3b                 done				RTI										;Return from interrupt
0188 e16c 14 a2 20           shiftOn				BSET		shiftFlag		%100000					;Shift on
0189 e16f 20 fa              				BRA		done								;Go to done
0190 e171 15 a2 20           shiftOff			BCLR		shiftFlag		%100000					;Shift off
0191 e174 20 f5              				BRA		done								;Go to done
0192 e176 7c 00 a3           modePlusPlus			INC		mode								;mode++
0193 e179 86 05              				LDAA		#5								;5 -> A
0194 e17b d6 a3              				LDAB		mode								;mode -> B
0195 e17d 11                 				CBA										;if (mode < 5)
0196 e17e 26 eb              				BNE		done								;Go to done
0197 e180 86 01              				LDAA		#1								;else
0198 e182 97 a3              				STAA		mode								;mode = 1
0199 e184 20 e5              				BRA		done								;Go to done
0200 e186 7a 00 a3           modeMinusMinus			DEC		mode								;mode--
0201 e189 86 00              				LDAA		#0								;0 -> A
0202 e18b d6 a3              				LDAB		mode								;mode -> B
0203 e18d 11                 				CBA										;if (mode > 0)
0204 e18e 26 db              				BNE		done								;Go to done
0205 e190 86 04              				LDAA		#4								;else
0206 e192 97 a3              				STAA		mode								;mode = 4
0207 e194 20 d5              				BRA		done								;Go to done
