0160 e12d 81 13              				CMPA		#%10011								;else if (keypress == mode && !shiftFlag)
0161 e12f 27 45              				BEQ		modePlusPlus							;mode++
0162 e131 81 23              				CMPA		#%100011							;else if (keypress == shift && shiftFlag)
